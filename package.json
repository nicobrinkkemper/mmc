{
  "name": "ymm",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://mmcelebration.com",
  "scripts": {
    "start": "run-s react:start",
    "lint": "eslint ./src --fix",
    "serve": "node server/dist/dev-server.mjs",
    "serve-gh": "PUBLIC_URL=/mmc node server/dist/dev-server.mjs",
    "build": "export BASE_URL='https://mmcelebration.com' && vite build",
    "server:build": "rm -rf dist && npx tsc --project tsconfig.node.json && cp --parents `find -name \"*.css\"` dist/",
    "server:run": "run-p server:rsc:dev server:ssr:dev",
    "server:iterate": "npm run server:build && run-p server:start",
    "server:start": "NODE_OPTIONS='--experimental-json-modules --enable-source-maps' node ./dist/server/start.mjs",
    "server:run:prod": "run-p server:rsc:prod server:ssr:prod",
    "server:rsc:dev": "NODE_OPTIONS='--conditions=react-server --enable-source-maps --experimental-json-modules --loader ./dist/server/moduleHook.dev.mjs' node ./dist/server/rsc-server.mjs",
    "server:rsc:prod": "NODE_OPTIONS='--conditions=react-server --experimental-json-modules --loader ./dist/server/moduleHook.prod.mjs' node ./dist/server/rsc-server.mjs",
    "server:ssr:dev": "NODE_OPTIONS='--experimental-json-modules --enable-source-maps --loader ./dist/server/moduleHook.dev.mjs' node ./dist/server/ssr-server.mjs",
    "server:ssr:prod": "NODE_OPTIONS='--experimental-json-modules --loader ./dist/server/moduleHook.prod.mjs' node ./dist/server/ssr-server.mjs",
    "server:crawl": "NODE_OPTIONS='--conditions=react-server --experimental-json-modules' node ./dist/server/server.mjs crawl",
    "server:export": "NODE_OPTIONS='--conditions=react-server --experimental-json-modules' node ./dist/server/export.mjs",
    "ssg": "run-s build server:build server:crawl",
    "build-gh": "export BASE_URL='https://nicobrinkkemper.github.io' && export PUBLIC_URL='/mmc' && run-s startup-gh react:build server:build server:crawl",
    "postbuild": "export BASE_URL='https://mmcelebration.com'",
    "prebuild-gh": "run-s startup:*",
    "postbuild-gh": "echo 'Build completed'",
    "postinstall": "cp -r oss-experimental/* node_modules/",
    "predeploy": "npm run build-gh",
    "deploy": "gh-pages -d build",
    "test": "run-s startup react:test",
    "react:test": "vitest",
    "react:build": "vite build",
    "react:start": "vite",
    "startup-gh": "run-s startup:*",
    "startup": "run-s startup:*",
    "startup:compile": "npm run server:build",
    "startup:run": "export NODE_ENV='development'; export NODE_OPTIONS='--experimental-json-modules --enable-source-maps'; node ./dist/startup/startup.mjs",
    "dev": "vite",
    "preview": "vite preview"
  },
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "src/**/*.ts?(x)",
          "startup/src/**/*.mts?(x)"
        ],
        "rules": {}
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.13",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.0-rc.1",
    "@types/react-dom": "^19.0.0-rc.1",
    "@vitejs/plugin-react": "^4.3.4",
    "acorn-loose": "^8.4.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-37ed2a7-20241206",
    "classnames": "^2.5.1",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "eslint-plugin-react-compiler": "^19.0.0-beta-37ed2a7-20241206",
    "express": "^4.21.2",
    "gh-pages": "^6.2.0",
    "happy-dom": "^15.11.7",
    "image-size": "^1.1.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "npm-check-updates": "^17.1.11",
    "npm-run-all": "^4.1.5",
    "postcss-modules": "^6.0.1",
    "react": "^19.0.0-beta-26f2496093-20240514",
    "react-dom": "^19.0.0-rc.1",
    "sharp": "^0.33.5",
    "typescript": "^5.7.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.0.3",
    "vitest": "^2.1.8",
    "webpack-sources": "^3.2.3"
  },
  "include": [
    "src"
  ]
}
