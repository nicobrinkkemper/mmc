{
  "name": "ymm",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://mmcelebration.com",
  "scripts": {
    "start": "run-s startup react:start",
    "lint": "eslint ./src --fix",
    "serve": "serve build",
    "build": "run-s startup react:build",
    "build-gh": "export PUBLIC_URL='https://nicobrinkkemper.github.io/7mmc'; npm run build",
    "postbuild": "react-snap",
    "predeploy": "npm run build-gh",
    "deploy": "gh-pages -d build",
    "test": "run-s startup react:testing",
    "react:test": "react-scripts test",
    "react:build": "react-scripts build",
    "react:start": "react-scripts start",
    "startup": "run-p lint startup:*",
    "startup:csv": "node ./scripts/csvToJson.mjs",
    "startup:resize": "node ./scripts/resize.mjs"
  },
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {}
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox"
    ],
    "inlineCss": false
  },
  "devDependencies": {
    "@babel/runtime": "^7.23.5",
    "@fullstory/browser": "^2.0.0",
    "@google-cloud/local-auth": "^3.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-gtm-module": "^2.0.3",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack-env": "^1.18.4",
    "classnames": "^2.3.2",
    "clipboard": "^2.0.11",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.0",
    "google-auth-library": "^9.4.1",
    "googleapis": "^129.0.0",
    "htmdx": "^0.3.7",
    "image-size": "^1.0.2",
    "jimp": "^0.22.10",
    "lodash": "^4.17.21",
    "memoize-one": "^6.0.0",
    "npm-check-updates": "^16.14.11",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.4.1",
    "prettier": "3.1.0",
    "puppeteer": "^21.6.0",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-app-polyfill": "^3.0.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-gtm-module": "^2.0.11",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.1",
    "react-snap": "^1.23.0",
    "responsive-loader": "^3.1.2",
    "serve": "^14.2.1",
    "sharp": "^0.33.0",
    "ts-node": "^10.9.2",
    "type-guard-helpers": "^1.1.21",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "url-loader": "^4.1.1",
    "web-vitals": "^3.5.0",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "typescript": "^5.3.3",
    "react": "$react",
    "react-dom": "$react-dom",
    "@babel/runtime": "$@babel/runtime"
  },
  "include": [
    "src"
  ]
}
