{
  "name": "ymm",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://mmcelebration.com",
  "scripts": {
    "start": "run-s react:start",
    "lint": "eslint ./src --fix",
    "serve": "serve build/",
    "prebuild": "npm run startup",
    "build": "export BASE_URL='https://mmcelebration.com' && react-scripts build",
    "server:build": "cd server && rm -rf dist && tsc --build --force",
    "server:run": "node server/dist/server.js",
    "server:crawl": "node --trace-warnings server/dist/server.js crawl",
    "ssg": "run-s build server:build server:crawl",
    "build-gh": "export BASE_URL='https://nicobrinkkemper.github.io' && export PUBLIC_URL='/mmc' && run-s react:build server:build server:crawl",
    "postbuild": "export BASE_URL='https://mmcelebration.com'",
    "prebuild-gh": "npm run startup-gh",
    "postbuild-gh": "echo 'Build completed'",
    "postinstall": "cd server && npm install",
    "predeploy": "npm run build-gh",
    "deploy": "gh-pages -d build",
    "test": "run-s startup react:test",
    "react:test": "react-scripts test",
    "react:build": "react-scripts build",
    "react:start": "react-scripts start",
    "startup-gh": "export PUBLIC_URL='/mmc' && run-s startup:*",
    "startup": "run-s startup:*",
    "startup:compile": "tsc --project tsconfig.node.json",
    "startup:run": "export NODE_OPTIONS=--enable-source-maps; node ./startup/dist/startup.mjs"
  },
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "src/**/*.ts?(x)",
          "startup/src/**/*.mts?(x)"
        ],
        "rules": {}
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "^2.0.31",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^19.0.0-rc.1",
    "@types/react-dom": "^19.0.0-rc.1",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack-env": "^1.18.4",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.0",
    "htmdx": "^0.3.7",
    "image-size": "^1.0.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "markdown-to-jsx": "^7.4.0",
    "npm-check-updates": "^16.14.11",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.4.1",
    "react": "^19.0.0-rc.1",
    "react-accessible-accordion": "^5.0.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^19.0.0-rc.1",
    "react-error-boundary": "^4.0.11",
    "react-gtm-module": "^2.0.11",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.3",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.1",
    "sharp": "^0.33.0",
    "type-guard-helpers": "^1.1.21",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "typescript": "^5.3.3",
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "include": [
    "src"
  ],
  "dependencies": {
    "jsdom": "^9.12.0"
  }
}
